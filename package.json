{
  "name": "craft-bp",
  "version": "2.2.0",
  "description": "Craft CMS boilerplate and starter project",
  "main": "index.js",
  "keywords": [
    "Craft",
    "Boilerplate",
    "TailwindCSS",
    "Start Project"
  ],
  "author": "Terry Upton, Amasci Creative Limited",
  "contact": "<terry@amasci.co.uk> <www.amasci.co.uk> <01908 668665>",
